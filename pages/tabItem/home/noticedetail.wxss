.gradientsContainer {
  background: linear-gradient(to right, #008ecd, #48b8d2);
  display: flex;
  align-items: center;
  justify-content: center;

}

.avatar {
  height:120rpx;
  width:120rpx;
  border-radius:50%;
  overflow: hidden;
  background-color: gainsboro;
  border:4rpx solid white;
}
.list_top{
  width:95%;
  background:#fff;
}
.List{
  width:100%;
  height:200rpx;
  background:#fff;
  margin-top:20rpx;
  display: flex;
  font-size:34rpx;
}
.listFlex{
  flex:1;
  align-items: center;
  justify-content: center;
}
.but{
  /* width:150rpx; */
  padding:10rpx 20rpx;
  background:#93D150;
  color:#fff;
  font-size:34rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius:5rpx;
  
}
.imageTop{
  position: fixed;
  top:0;
  left:0;
  width: 100%;
  height:300rpx;
  z-index:1000;

}
.imageTop image{
  width: 100%;
  height:300rpx;
}
